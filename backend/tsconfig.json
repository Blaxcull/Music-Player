{
  "compilerOptions": {
    "target": "ES2020",

"module": "Node16",
"moduleResolution": "Node16",

    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,

    "types": ["node", "@vercel/node"],

    "rootDir": ".",
    "outDir": "dist"
  },
  "include": ["api/**/*.ts", "config/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}

